@inproceedings{10.1145/3368089.3409676,
author = {Zhang, Ziqi and Li, Yuanchun and Guo, Yao and Chen, Xiangqun and Liu, Yunxin},
title = {Dynamic slicing for deep neural networks},
year = {2020},
isbn = {9781450370431},
